CommentEnd = :)
StringStart = "
StringEnd = "
OperatorChars = -/<>.!~%^&|=@$:
StringEsc = \
CharStart = '
CharEnd = '
CharEsc = \


; Note: the following are duplicate HTML keywords and are found in both
